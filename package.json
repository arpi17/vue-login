    "core-js": "^3.1.4",
    "express": "^4.17.1",
    "jsonwebtoken": "^8.5.1",
    "lodash.isempty": "^4.4.0",
    "mongoose": "^5.5.14",
    "validator": "^11.1.0",
    "vue": "^2.6.10",
    "vue-router": "^3.0.7",
    "vuex": "^3.0.1"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.8.0",
    "@vue/cli-plugin-eslint": "^3.9.2",
    "@vue/cli-service": "^3.8.0",
    "@vue/eslint-config-prettier": "^4.0.1",
    "babel-eslint": "^10.0.2",
    "concurrently": "^4.1.0",
    "eslint": "^5.16.0",
    "eslint-plugin-vue": "^5.2.3",
    "nodemon": "^1.19.1",
