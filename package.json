{
  "name": "vue-login",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "yarn": "1.x"
  },
  "scripts": {
    "start": "node server",
    "server": "nodemon server",
    "client": "vue-cli-service serve --open",
